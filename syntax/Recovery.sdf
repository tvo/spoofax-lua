module Recovery
imports
  Common
  Statements

exports
  context-free syntax

    %% Don't choke too badly on misplaced break/return statements
    LastStat -> Stat {recover}

    %% Helps content completion
    COMPLETION -> Stat   {cons("WATER")}
    COMPLETION -> ElseIf {cons("WATER")}
    COMPLETION -> Else   {cons("WATER")}
